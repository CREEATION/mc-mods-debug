{
  "version": "1.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "tauri": "tauri",
    "tauri-init": "wait-on http://localhost:5000 && npm run tauri dev",
    "tauri-icon": "npm run tauri icon --icon ./public/favicon.png",
    "tauri-dev": "concurrently \"npm run dev\" \"npm run tauri-init\"",
    "tauri-build": "npm run tauri build",
    "test": "nyc --reporter=text --reporter=json-summary jasmine-ts",
    "test-coverage": "npm test && istanbul-badges-readme",
    "postinstall": "concurrently \"npm run tauri info\" \"npm run tauri-icon\""
  },
  "prettier": "@creeation/prettier-config",
  "nyc": {
    "exclude": [
      "rollup.config.js",
      "spec",
      "src-tauri"
    ],
    "extends": "@istanbuljs/nyc-config-typescript"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.0-beta.1",
    "@tauri-apps/cli": "^1.0.0-beta.2",
    "sirv-cli": "^1.0.12"
  },
  "devDependencies": {
    "@creeation/prettier-config": "^1.2.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@tsconfig/svelte": "^2.0.1",
    "@types/jasmine": "^3.7.7",
    "concurrently": "^6.2.0",
    "istanbul-badges-readme": "^1.3.4",
    "jasmine": "^3.7.0",
    "jasmine-core": "^3.7.1",
    "jasmine-ts": "^0.3.3",
    "normalize-path": "^3.0.0",
    "nyc": "^15.1.0",
    "rollup": "^2.50.6",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.34.1",
    "slug": "^5.0.1",
    "svelte": "^3.38.2",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.7.3",
    "ts-node": "^10.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.3.2",
    "wait-on": "^5.3.0"
  }
}
